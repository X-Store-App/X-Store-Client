{
  "name": "xstore-electron-client",
  "productName": "XStore Desktop",
  "version": "1.1.3",
  "description": "Client for XStore",
  "main": "bin/main.js",
  "repository": "https://github.com/X-Store-App/client",
  "author": "dragonDScript",
  "license": "MIT",
  "types": "./src/types/global.d.ts",
  "bugs": {
    "url": "https://github.com/X-Store-App/client/issues"
  },
  "private": true,
  "scripts": {
    "dev": "npm run build:webpack:main && run-p watch:webpack:renderer start",
    "start": "electron-forge start",
    "package": "npm run build:webpack && electron-forge package",
    "make": "electron-forge make",
    "build:webpack": "webpack --config webpack.config.js --mode=production",
    "build:webpack:test": "webpack --config webpack.config.js --mode=production --env.NODE_ENV=test",
    "build:webpack:main": "webpack --config webpack.config.js --mode=development --config-name preload && webpack --mode=development --config-name main",
    "build:webpack:renderer": "webpack --config webpack.config.js --mode=production --config-name renderer",
    "watch:webpack:renderer": "webpack-dev-server --config webpack.config.js --mode=development --config-name renderer",
    "lint": "eslint ./**/*{.js,.ts,.tsx,.jsx} --fix"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "@types/electron": "^1.6.10",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "@webpack-cli/serve": "^1.1.0",
    "babel-loader": "^8.1.0",
    "css-loader": "^5.0.1",
    "electron": "^11.1.0",
    "eslint": "^7.12.1",
    "eslint-config-standard": "^16.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.21.5",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.0",
    "npm-run-all": "^4.1.5",
    "parallel-webpack": "^2.6.0",
    "postcss": "^8.2.1",
    "postcss-loader": "^4.1.0",
    "postcss-preset-env": "^6.7.0",
    "react": "^16.13.1",
    "spectron": "^12.0.0",
    "style-loader": "^2.0.0",
    "svg-loader": "0.0.2",
    "ts-loader": "^8.0.10",
    "typescript": "^4.0.3",
    "webpack": "^5.10.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-html-plugin": "^0.1.1"
  },
  "dependencies": {
    "@xstoreapp/api": "^1.0.0",
    "@xstoreapp/react-design": "^0.2.0",
    "electron-is-dev": "1.2.0",
    "electron-log": "^4.3.0",
    "electron-squirrel-startup": "^1.0.0"
  },
  "config": {
    "forge": "./forge.config.js"
  }
}
